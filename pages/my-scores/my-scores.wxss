.timetextbox {
    text-align: center;
    width: 100%;
    background-image: url(http://love.imtudou.cn/img/my-jf-banner.jpg);
    background-size: 100% 100%;
    height: 330rpx;
}

.timetextbox view:nth-of-type(1) {
    font-size: 65rpx;
    height: 55rpx;
    line-height: 233rpx;
    color: #fff;
}

.timetextbox view:nth-of-type(2) {
    font-size: 30rpx;
    height: 175rpx;
    line-height: 260rpx;
    color: #fff;
}

.boxlist {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.boxitem {
    width: 90%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-direction: row;
    border-bottom: 1px solid #eaeaea;
    padding: 0.8rem 0.5rem;
}

.boxitem-left {
    display: flex;
    flex-direction: column;
}

.boxitem-left text:nth-of-type(1) {
    font-size: 32rpx;
    height: 40rpx;
    line-height: 40rpx;
}

.boxitem-left text:nth-of-type(2) {
    font-size: 28rpx;
    height: 28rpx;
    line-height: 36rpx;
    color: rgb(192, 192, 192);
}

.boxitem-right {
    color: #e50026;
}
.scores-footer{
  height:70rpx;
  text-align:center;
  font-size:28rpx;
  margin:40rpx 20rpx;
}