.timetextbox {
    text-align: center;
    width: 100%;
    background-image: url(http://love.imtudou.cn/img/my-zysc-banner.jpg);
    background-size: 100% 100%;
    height: 390rpx;
    margin-top: -10rpx;
}

.timetextbox view:nth-of-type(1) {
    font-size: 65rpx;
    height: 202rpx;
    line-height: 333rpx;
    color: #fff;
}

.timetextbox view:nth-of-type(2) {
    font-size: 30rpx;
    height: 30rpx;
    line-height: 100rpx;
    color: #fff;
}

.spanview {
    height: 0.5rem;
    background-color: #f1f1f1;
}

.boxlist {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.boxitem {
    width: 90%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    flex-direction: row;
    border-bottom: 1px solid #eaeaea;
    padding: 0.8rem 0.5rem;
}

.boxitem-left {
    display: flex;
    flex-direction: column;
}

.boxitem-left text:nth-of-type(1) {
    font-size: 32rpx;
    height: 40rpx;
    line-height: 40rpx;
}

.boxitem-left text:nth-of-type(2) {
    font-size: 28rpx;
    height: 28rpx;
    line-height: 36rpx;
    color: rgb(192, 192, 192);
}

.boxitem-right {
    color: #e50026;
}