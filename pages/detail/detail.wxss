    font-size:24rpx;
    color:#333;
    border-bottom:1rpx solid #ebebeb;
    padding:30rpx;
    line-height:45rpx;
}
.detail-main-box{
    background-color:#fff;
}
.detail-title{
    display:flex;
    justify-content:space-between;
    height:90rpx;
    line-height:90rpx;
    font-size:36rpx;
    color:#218ccd;
    padding:10rpx;
    border-bottom:1rpx solid #ebebeb;
}
.detail-title-left{
    margin-left:20rpx;
}
.detail-title-right{
    color:#333;
    font-size:24rpx;
    margin-right:20rpx;
}
.detail-bottom-box{
    background-color:#fff;
}
.detail-content{
    display:flex;
    justify-content:space-between;
    padding:30rpx;
}
.select-dz{
    background-color:#e50026;
}
.detail-content-img{
    width:75rpx;
    height:75rpx;
    border-radius:50%;
}
.detail-phone{
    flex:1.5;
    margin-left:15rpx;
}
.detail-phone text{
    font-size:24rpx;
    color:#333;
    display:block;
    margin-top:10rpx;
}
.detail-code{
    flex:1.5;
    margin-left:45rpx;
}
.detail-code image {
    width:216rpx;
    height:216rpx;

}
.signup_item{
    display:flex;
    justify-content:space-between;
    height:60rpx;
