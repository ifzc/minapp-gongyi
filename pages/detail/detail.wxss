    color:#999;
    border-bottom:1rpx solid #ebebeb;
}
.detail-infos text{
    color:#333;
    margin-left:15rpx;
}
.start-time{
    display:flex;
    justify-content:space-between;
    margin-top:10rpx;
}
.detail-status{
    width:110rpx;
    height:60rpx;
    background-color:#218ccd;
    line-height:60rpx;
    text-align:center;
    color:#fff;
    font-size:24rpx;
    margin-top:-38rpx;
}
.end-time{
    margin-top:16rpx;
}
.perpon{
    display:flex;
    justify-content:space-between;
    margin-top:10rpx;
}
.perpon .icon-dianz{
    width:70rpx;
    height:70rpx;
    background-color:#f1f1f1;
    border-radius:50%;
    display:flex;
    justify-content:center;
    align-items:center;
    margin-top:-38rpx;

}
.perpon .icon-dianz image {
    width:35rpx;
    height:35rpx;

}
.detail-type{
    display:flex;
    justify-content:space-between;
